 {
    "none" : {
      "type" : "keyword"
    },
    "coma" : {
      "type" : "pattern",
      "lowercase" : false,
      "pattern" : ", "
    },
    "semicolon" : {
      "type" : "pattern",
      "lowercase" : false,
      "pattern" : "; "
    },
    "default" : {
      "type" : "custom",
      "tokenizer": "standard",
      "filter":  [ "lowercase", "stop", "asciifolding", "english_stemmer" ]
    },
    "date2year" : {
      "type" : "pattern",
      "pattern" : "[-](.*)"
    },
    "didYouMean": {
      "filter": [
        "lowercase"
      ],
      "char_filter": [
        "html_strip"
      ],
      "type": "custom",
      "tokenizer": "standard"
    },
    "autocomplete": {
      "filter": [
        "lowercase", "stop", "autocompleteFilter", "trim"
      ],
      "char_filter": [
        "html_strip"
      ],
      "type": "custom",
      "tokenizer": "standard"
    },
    "pipe" : {
      "type" : "pattern",
      "lowercase" : false,
      "pattern" : "\\|"
    },
    "freetext": {
      "filter": [
        "lowercase"
      ],
      "char_filter": [
        "html_strip"
      ],
      "type": "custom",
      "tokenizer": "standard"
    }
}
